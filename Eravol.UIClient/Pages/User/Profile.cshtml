                                                            <li class="wt-uploadingholder">
                                                                <div class="wt-uploadingbox">
                                                                    <div class="wt-designimg">
                                                                        <input id="avatar-@image.ImgageId" type="radio" name="userAvatar" checked="">
                                                                        <label for="demoq"><img src=@($"https://localhost:7259/api/Images/{image.UserImageName}") alt="img description"><i class="fa fa-check"></i></label>
                                                                    </div>
                                                                    <div class="wt-uploadingbar">
                                                                        <span class="uploadprogressbar"></span>
                                                                        <span>Banner Photo.jpg</span>
