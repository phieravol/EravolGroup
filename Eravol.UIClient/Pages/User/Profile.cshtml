@using Eravol.UserWebApi.Data.Models;
@using Eravol.WebApi.Data.Models;
@using Eravol.WebApi.ViewModels.UserSkills;
@model Eravol.UIClient.Pages.User.ProfileModel
@{
    AppUser? appUser = Model.appUser;
    string? token = Model.token;
    List<UserImage>? profileImages = Model.profileImages;
    List<UserImage>? userAvatars = Model.userAvatars;
    List<Skill>? skills = Model.Skills;
    List<UserSkillViewModel>? mySkills = Model.UserSkills;
}
<head>
    <link rel="apple-touch-icon" href="apple-touch-icon.png">
    <link rel="icon" href="images/favicon.png" type="image/x-icon">
    <link rel="stylesheet" href="~/css/themify-icons.css">
    <link rel="stylesheet" href="~/css/dashboard.css">
    <link rel="stylesheet" href="~/css/dbresponsive.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <script src="~/js/vendor/modernizr-2.8.3-respond-1.4.2.min.js"></script>
    
</head>
<!--Main Start-->
<main id="wt-main" class="wt-main wt-haslayout">
    
    <!--Sidebar Start-->
    @await Component.InvokeAsync("DashboardSideBarComponent")
    <!--Sidebar Start-->
    <!--Register Form Start-->
    <section class="wt-haslayout">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-8 col-xl-9">
                <div class="wt-haslayout wt-dbsectionspace">
                    <div class="wt-dashboardbox wt-dashboardtabsholder">
                        <div class="wt-dashboardboxtitle">
                            <h2>My Profile</h2>
                        </div>
                        <div class="wt-dashboardtabs">
                            <ul class="wt-tabstitle nav navbar-nav">
                                <li class="nav-item">
                                    <a class="active" data-toggle="tab" href="#wt-skills">Personal Details &amp; Skills</a>
                                </li>
                                <li class="nav-item"><a data-toggle="tab" href="#wt-education">Experience &amp; Education</a></li>
                                <li class="nav-item"><a data-toggle="tab" href="#wt-awards">Projects &amp; Awards</a></li>
                            </ul>
                        </div>
                        <div class="wt-tabscontent tab-content">
                            <div class="wt-personalskillshold tab-pane active fade show" id="wt-skills">
                                <div class="wt-yourdetails wt-tabsinfo">
                                    <div class="wt-tabscontenttitle">
                                        <h2>Your Details</h2>
                                    </div>
                                    <form class="wt-formtheme wt-userform">
                                        <fieldset>
                                            <input type="hidden" id="userId" name="userId" value="@appUser.Id">
                                            <input type="hidden" id="token_Js" name="token_Js" value="@token">
                                            <div class="form-group form-group-half">
                                                <input type="text" asp-for="@appUser.FirstName" id="input-firstname_Js" class="form-control" placeholder="First Name">
                                            </div>
                                            <div class="form-group form-group-half">
                                                <input type="text" asp-for="@appUser.LastName" id="input-lastname_Js" class="form-control" placeholder="Last Name">
                                            </div>
                                            <div class="form-group form-group-half">
                                                <input type="text" asp-for="@appUser.PhoneNumber" id="input-phone_Js" class="form-control" placeholder="Phone Number">
                                            </div>
                                            <div class="form-group form-group-half">
                                                <input type="date" asp-for="@appUser.Birthday" id="input-birthdat_Js" class="form-control" placeholder="Date of birth">
                                            </div>
                                            <div class="form-group">
                                                <input type="text" asp-for="@appUser.Tagline" id="input-tagline_Js" class="form-control" placeholder="Add Your Tagline Here">
                                            </div>
                                            <div class="form-group">
                                                <textarea name="description" id="text-desc_Js" class="form-control" placeholder="Description">@appUser.Description</textarea>
                                            </div>
                                        </fieldset>
                                    </form>
                                </div>
                                <div class="wt-profilephoto wt-tabsinfo">
                                    <div class="wt-tabscontenttitle">
                                        <h2>Profile Photos</h2>
                                    </div>
                                    <div class="wt-profilephotocontent">
                                        <div class="wt-description">
                                            <p>Select images to add into your profile photo.</p>
                                        </div>
                                        <form class="wt-formtheme wt-formprojectinfo wt-formcategory" enctype="multipart/form-data">
                                            <fieldset>
                                                <div class="form-group form-group-label">
                                                    <div class="wt-labelgroup">
                                                        <label for="filep">
                                                            <span class="wt-btn">Select Files</span>
                                                            <input type="file" name="file" id="filep" multiple accept=".jpg, .jpeg, .png">
                                                        </label>
                                                        <span>Drop files here to upload</span>
                                                        <em class="wt-fileuploading">Uploading<i class="fa fa-spinner fa-spin"></i></em>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <ul class="wt-attachfile wt-attachfilevtwo" id="profileImgContainer">
                                                        @foreach (var image in profileImages)
                                                        {
                                                            <li class="wt-uploadingholder" id="item-profileImg_@image.ImgageId">
                                                                <div class="wt-uploadingbox">
                                                                    <div class="wt-designimg">
                                                                        <label for="demoz"><img src=@($"https://localhost:7259/api/Images/{image.UserImageName}") alt="img description"></label>
                                                                    </div>
                                                                    <div class="wt-uploadingbar wt-uploading">
                                                                        <span class="uploadprogressbar"></span>
                                                                        <em>File size: @(image.UserImageSize / 1024) kb<button type="button" onclick="removeProfileImage(@image.ImgageId)" class="lnr lnr-cross" style="color:red"></button></em>
                                                                    </div>
                                                                </div>
                                                            </li>
                                                        }
                                                    </ul>
                                                </div>
                                            </fieldset>
                                        </form>
                                    </div>
                                </div>
                                <div class="wt-bannerphoto wt-tabsinfo">
                                    <div class="wt-tabscontenttitle">
                                        <h2>My Avatar</h2>
                                    </div>
                                    <div class="wt-profilephotocontent">
                                        <div class="wt-description">
                                            <p>Select photo from computer to change your avatar.</p>
                                        </div>
                                        <form class="wt-formtheme wt-formprojectinfo wt-formcategory">
                                            <fieldset>
                                                <div class="form-group form-group-label">
                                                    <div class="wt-labelgroup">
                                                        <label for="filew">
                                                            <span class="wt-btn">Select Files</span>
                                                            <input type="file" name="file" id="filew">
                                                        </label>
                                                        <span>Drop files here to upload</span>
                                                        <em class="wt-fileuploading">Uploading<i class="fa fa-spinner fa-spin"></i></em>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <ul class="wt-attachfile wt-attachfilevtwo" id="user-avatar_img">
                                                        @foreach (var image in userAvatars)
